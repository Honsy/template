  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm-run-all -p watch",
    "build": "npm-run-all build-dev",
    "build-dev": "npm-run-all build:js build:css",
    "build:js": "rollup -c",
    "build:css": "npm-run-all build:css:*",
    "build:css:default": "sass --load-path='./' --no-source-map src/css/designer.scss dist/designer.css",
    "watch": "npm-run-all -p watch:*",
    "watch:rollup": "rollup -c -w --no-progress",
    "watch:css": "npm-run-all -p build:css:default watch:css:*",
    "watch:css:default": "npm run build:css:default -- --watch",
    "designer": "",
    "lint": "eslint"
  },
